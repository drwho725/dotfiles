{
    "default_module": "powerline.segments.common",
    "segment_data": {
        "date": {
            "before": ""
        },
        "time": {
            "before": ""
        }
    },
    "segments": {
        "right": [
            {
                "function": "system_load",
                "priority": 50
            },
            {
                "function": "date"
            },
            {
                "function": "date",
                "name": "time",
                "args": {
                    "format": "%H:%M",
                    "istime": true
                }
            }
        ]
    }
}
